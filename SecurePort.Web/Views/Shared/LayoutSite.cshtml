<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv='Content-Type' content='text/html; charset=iso-8859-1' />
		<meta charset="utf-8" />
		<meta http-equiv='X-UA-Compatible' content="IE=edge,IE=9,IE=8,chrome=1" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">
		<meta content="" name="description" />
		<meta content="" name="author" />
		<meta http-equiv="Expires" content="0" />
		<meta http-equiv="'Cache-Control: no-store, no-cache, must-revalidate, post-check=0, pre-check=0'" content="no-cache" />
		@using System.Web.Optimization;
		<title>@(this.ViewBag.Title) - SecurePort</title>
		 @Styles.Render("~/Content/Telerik/css")
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/iCheck/skins/all.css">
		<link rel="stylesheet" href="~/Scripts/SecurePort/plugins/bootstrap/css/bootstrap.css"  media="screen">
		<link rel="stylesheet" href="~/Content/css/theme_light.css" id="skin_color">
		<link rel="stylesheet" href="~/Content/css/main-responsive.css">
		<link rel="stylesheet" href="~/Content/css/base.css">
		<link rel="stylesheet" href="~/Content/css/style.css">
		<link rel="stylesheet" href="~/Content/css/main.css">
		<link rel="stylesheet" href="~/Content/css/loading.css">
		<link rel="stylesheet" href="~/Scripts/SecurePort/plugins/font-awesome/css/font-awesome.min.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/select2/select2.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/perfect-scrollbar/src/perfect-scrollbar.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/datepicker/css/datepicker.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/bootstrap-modal/css/bootstrap-modal.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/ckeditor/contents.css">
		<link rel="stylesheet" href="~/Scripts/secureport/plugins/ladda-bootstrap/dist/ladda-themeless.min.css">
		@Scripts.Render("~/bundles/modernizr")
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
		<script src="http://kendo.cdn.telerik.com/2016.1.112/js/jszip.min.js"></script>
		@(this.RenderBody())
		<div id="alert">
			<span id="message">Error</span>
			<button class="blue" type="submit">OK</button>
		</div>
		<div id="confirm">
			<span id="message">Error</span>
			<button class="blue" id="yes">Si</button>
			<button class="red" id="no">No</button>
		</div>
		<span id="width"></span><br>
		<span id="height"></span>
		@Scripts.Render("~/bundles/jquery")
		@Scripts.Render("~/bundles/Telerik")
		@Scripts.Render("~/bundles/uploadify")
		@(this.RenderSection("scripts",required: false))
	</head>
	<body>
		<div id="Idloading" class="cssload-loading" style="display: none">Espere, cargando datos. </div>
		<div id="IdLogin" ></div>
		<section id="SeccionWhite" class="lightbox white">
			<div id="loading-page" style="display: none">
				<div class="bg-grey transparent">
				</div>
			</div>
		</section>	
		<script>
			jQuery(document).ready(function () {
				Main.init();
				FormElements.init();
				UIModals.init();
				UIElements.init();
				if (window.screen.availWidth == 1920) {
				    window.parent.document.body.style.zoom = "100%";
				} 
			});
		</script>
	</body>
</html>